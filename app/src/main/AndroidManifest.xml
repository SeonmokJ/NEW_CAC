<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NEW_CAC">

        <activity android:name=".BaseActivity"/>
        <activity android:name=".Settings"/>
        <activity android:name=".TeachLIB"/>
        <activity android:name=".TeachLIBmath"/>
        <activity android:name=".TeachLIBscience"/>
        <activity android:name=".TeachLIBenglish"/>
        <activity android:name=".TeachLIBhistory"/>
        <activity android:name=".TeachLIBcoding"/>
        <activity android:name=".Timer"/>
        <activity android:name=".KnowledgeVault"/>
        <activity
            android:name=".NotesListActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".NoteDetailActivity" />

        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>